{
  "name": "weaveworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "build-safe": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "aos-s": "func() { aos ${1} --wallet ./.secret/deploy_sqlite_jwk.json --module=\"GYrbbe0VbHim_7Hi6zrOpHQXrSQz07XNtwCnfbFo2I0\" ; }; func",
    "aos-s-load": "func() { aos ${1} --wallet ./.secret/deploy_sqlite_jwk.json --module=\"GYrbbe0VbHim_7Hi6zrOpHQXrSQz07XNtwCnfbFo2I0\" --load ${2} ; }; func",
    "aos-s-load-verse": "func() { npm run aos-s-load ${1} process/blueprint/Verse.lua ; }; func",
    "aos-s-load-chat": "func() { npm run aos-s-load ${1} process/blueprint/Chat.lua ; }; func",
    "aos-s-load-specific": "func() { npm run aos-s-load ${1} process/${2}_${1}.lua ; }; func",
    "deploy-all-verse": "concurrently \"npm:aos-s-load-verse Singularity\" \"npm:aos-s-load-verse Universe\" \"npm:aos-s-load-verse WeaveWorld\" \"npm:aos-s-load-verse OriginIsland\" \"npm:aos-s-load-verse LlamaFed\"",
    "deploy-all-chat": "concurrently \"npm:aos-s-load-chat WeaveWorld\" \"npm:aos-s-load-chat OriginIsland\" \"npm:aos-s-load-chat LlamaFed\"",
    "deploy-all-specific": "concurrently \"npm:aos-s-load-specific Singularity 0\" \"npm:aos-s-load-specific Universe 1\" \"npm:aos-s-load-specific WeaveWorld 2\" \"npm:aos-s-load-specific OriginIsland 3\" \"npm:aos-s-load-specific LlamaFed 4\" \"npm:aos-s-load-specific LlamaAssistant 5\"",
    "deploy-all": "concurrently \"npm:deploy-all-verse\" \"npm:deploy-all-chat\" \"npm:deploy-all-specific\"",
    "aos-l": "func() { aos ${1} --wallet ./.secret/deploy_llama_jwk.json --module=\"1PdCJiXhNafpJbvC-sjxWTeNzbf9Q_RfUNs84GYoPm0\" ; }; func"
  },
