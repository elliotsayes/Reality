    "@tanstack/react-router": "^1.34.3",
    "@xstate/react": "^4.1.1",
    "arweave": "^1.15.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "fontfaceobserver": "^2.3.0",
